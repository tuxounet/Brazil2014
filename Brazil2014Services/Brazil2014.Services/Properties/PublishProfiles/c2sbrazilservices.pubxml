  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)BRAZIL2014Entities-Web.config Connection String">
      <UpdateDestWebConfig>False</UpdateDestWebConfig>
